{
  "name": "node-frame",
  "version": "1.0.0",
  "description": "A lightweight, Express-like web framework for Node.js built with TypeScript",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "start": "node dist/examine/index.js",
    "dev": "node -r ts-node/register ./examine/server.ts",
    "watch": "nodemon",
    "build:watch": "tsc --watch",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepublishOnly": "rm -rf dist && npm run build"
  },
  "keywords": ["web-framework", "express", "typescript", "nodejs", "http", "server", "middleware"],
  "author": "Taha Mohamed",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/taha7/node-frame.git"
  },
  "bugs": {
    "url": "https://github.com/taha7/node-frame/issues"
  },
  "homepage": "https://github.com/taha7/node-frame#readme",
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "type": "commonjs",
  "devDependencies": {
    "@types/node": "^24.10.0",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
